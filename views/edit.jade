extends layout2
block content
  h2 Edit Project
  form(method ='post', action='/admin/new' enctype="multipart/form-data")
    .form-group
      label Title
        input.form-control(type='text', name ='title' value='#{row.title}')
    .form-group
      label Description
        input.form-control(type='text', name ='description' value='#{row.description}')
    .form-group
      label Client
        input.form-control(type='text', name ='client' value='#{row.client}')  
    .form-group
      label Service
        input.form-control(type='text', name ='service' value='#{row.service}')
    .form-group
      label Project Image
        input.form-control(type='file', name ='projectimage' value='#{row.image}')
    button.btn.btn-default(type='submit') Submit
          
                    
